{
        "extends": "wikimedia",
        "plugins": [
                "mediawiki"
        ],
        "env": {
                "browser": true,
                "jquery": true,
                "commonjs": true
	},
	"globals": {
		"dataValues": false,
		"util": false
	},
	"rules": {
		"mediawiki/valid-package-file-require": "error",
		"computed-property-spacing": "off",
		"indent": "off",
		"keyword-spacing": "off",
		"new-parens": "off",
		"no-underscore-dangle": "off",
		"one-var": "off",
		"operator-linebreak": "off",
		"space-before-function-paren": "off",
		"vars-on-top": "off"
	}
}
